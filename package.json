{
    "name": "nada-auth-v2",
    "scripts": {
        "dev": "wrangler dev --live-reload --env=dev",
        "deploy": "wrangler deploy --minify",
        "prebuild": "bun --env-file=./.prod.vars run ./src/scripts/register.ts",
        "prebuild-dev": "bun --env-file=./.dev.vars run ./src/scripts/register.ts",
        "lint-staged": "lint-staged",
        "tsc": "tsc",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,mjs,cjs,ts,tsx}": [
            "prettier --write",
            "eslint"
        ]
    },
    "dependencies": {
        "@discordjs/rest": "^2.4.2",
        "@hono/valibot-validator": "^0.5.2",
        "crypto-browserify": "^3.12.1",
        "discord-api-types": "^0.37.116",
        "discord-hono": "^0.11.2",
        "google-auth-library": "^9.15.0",
        "hono": "^4.6.17",
        "valibot": "^1.0.0-beta.12"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250121.0",
        "@eslint/js": "^9.18.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.1",
        "@types/bun": "^1.1.17",
        "@types/whatwg-mimetype": "^3.0.2",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-import-access": "^2.2.2",
        "expect-type": "^1.1.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.1",
        "prettier": "^3.4.2",
        "type-fest": "^4.32.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.21.0",
        "wrangler": "^3.103.2"
    },
    "patchedDependencies": {
        "discord-api-types@0.37.115": "patches/discord-api-types@0.37.115.patch",
        "google-auth-library@9.15.0": "patches/google-auth-library@9.15.0.patch",
        "type-fest@4.32.0": "patches/type-fest@4.32.0.patch"
    }
}
