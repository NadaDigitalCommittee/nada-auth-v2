{
    "name": "nada-auth-v2",
    "scripts": {
        "dev": "wrangler dev --live-reload --env=dev",
        "deploy": "wrangler deploy --minify",
        "prebuild": "bun --env-file=./.prod.vars run ./src/scripts/register.ts",
        "prebuild-dev": "bun --env-file=./.dev.vars run ./src/scripts/register.ts",
        "check-outdated-patches": "bun run ./src/scripts/check-outdated-patches.ts",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "lint": "bun lint:prettier && bun lint:eslint",
        "lint-staged": "lint-staged",
        "tsc": "tsc",
        "prepare": "husky"
    },
    "lint-staged": {
        "*": [
            "prettier --write --ignore-unknown"
        ],
        "*.{js,mjs,cjs,ts,tsx}": [
            "eslint"
        ]
    },
    "dependencies": {
        "@discordjs/rest": "^2.4.2",
        "@hono/valibot-validator": "^0.5.2",
        "crypto-browserify": "^3.12.1",
        "discord-api-types": "^0.37.117",
        "discord-hono": "^0.11.4",
        "google-auth-library": "^9.15.0",
        "hono": "^4.6.18",
        "valibot": "^1.0.0-beta.14"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250124.3",
        "@eslint/js": "^9.19.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.1",
        "@types/bun": "^1.2.0",
        "@types/whatwg-mimetype": "^3.0.2",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-import-access": "^2.2.2",
        "expect-type": "^1.1.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.2",
        "prettier": "^3.4.2",
        "prettier-plugin-toml": "^2.0.1",
        "type-fest": "^4.33.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.21.0",
        "wrangler": "^3.105.1"
    },
    "patchedDependencies": {
        "discord-api-types@0.37.117": "patches/discord-api-types@0.37.117.patch",
        "google-auth-library@9.15.0": "patches/google-auth-library@9.15.0.patch",
        "type-fest@4.33.0": "patches/type-fest@4.33.0.patch"
    }
}
